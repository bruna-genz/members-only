<div class=list-container>
    <h1 class="big-title">All users</h1>
    <%= will_paginate @collection, renderer: WillPaginateSemanticUi::ActionView::Renderer %>
    <ul class="users">
    <% @users.each do |user| %>
        <%= render user %>
    <% end %>
    </ul>
    
    <%= will_paginate @collection, renderer: WillPaginateSemanticUi::ActionView::Renderer %>
</div>